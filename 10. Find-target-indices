const targetIndices = (nums, target)=> {
    const n = nums.length;
    let indiceArray = new Array();
    
    const sort = (arr) => {
        let n = arr.length;
        let temp;
        for(let i = 0; i<n; i++){
            for(let j = 0; j<n-1; j++){
                if (arr[j] > arr[j + 1]) {
                    temp = arr[j];
                    arr[j] = arr[j + 1];
                    arr[j + 1] = temp;
                }
            }
        }
        return arr;
    }
    
    sort(nums)
    for(let i = 0; i<n; i++){
        if(nums[i] == target){
            indiceArray.push(i)
        }
    }
    sort(indiceArray)
    return indiceArray
};
