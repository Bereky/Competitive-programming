const isValid = (s)=> {
    let bracketStack = new Array;
    let top = -1;

    if(s.length >=1 && s.length <= Math.pow(10, 4)){
        for(let i in s){
            if(s[i] === '{' || s[i] === '(' || s[i] === '['){
                bracketStack.push(s[i])
                top+=1
            }
            else{
                if(bracketStack[top] === '{' && s[i] === '}'){
                    bracketStack.pop()
                    top-=1

                }
                else if(bracketStack[top] === '(' && s[i] === ')'){
                    bracketStack.pop()
                    top-=1
                }
                else if(bracketStack[top] === '[' && s[i] === ']'){
                    bracketStack.pop()
                    top-=1
                }
                else {
                    return false
                }
            }
        }

        if(bracketStack.length === 0){
            return true
        }
        else {
            return false
        }
    }      
};
