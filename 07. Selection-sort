const selectionSort = (arr) => {
    let n = arr.length;
    let temp;
    let minimum;
    
    if(n<=1000){
        for(let i = 0; i<n-1; i++){
            minimum = i;
            for(let j = i+1; j<n; j++){
                if(arr[minimum] > arr[j]){
                    minimum = j;
                }
            }
            temp = arr[minimum];
            arr[minimum] = arr[i];
            arr[i] = temp;
        }
        console.log(...arr)
    }
}
